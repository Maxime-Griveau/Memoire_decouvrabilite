  title = {Orchestrating {{Overviews}}: {{A Synoptic Approach}} to the {{Visualization}} of {{Cultural Collections}}},
  shorttitle = {Orchestrating {{Overviews}}},
  author = {Windhager, Florian and Salisu, Saminu and Schreder, Günther and Mayr, Eva},
